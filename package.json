    "@types/eslint": "^8",
    "@types/micro-cors": "^0.1.3",
    "@types/nodemailer": "^6",
    "@types/prettier": "^2",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "prisma": "^4.14.0",
    "@graphql-codegen/client-preset": "3.0.1"
  },
  "volta": {
    "node": "18.16.0"
